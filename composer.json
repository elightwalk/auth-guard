{
    "name": "elightwalk/authguard",
    "description": "Manage laravel auth guard",
    "keywords": [
        "laravel guard",
        "laravel_module",
        "guard",
        "php",
        "elightwalk"
    ],
    "license": "MIT",
    "authors": [
        {
        "name": "Elightwalk Technology",
        "email": "hello@elightwalk.com",
        "homepage": "https://elightwalk.com",
        "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "elightwalk/laravel-modules": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0|~8.0",
        "mockery/mockery": "~1.0",
        "orchestra/testbench": "^4.0",
        "friendsofphp/php-cs-fixer": "^2.15",
        "laravel/framework": "6.0.*",
        "spatie/phpunit-snapshot-assertions": "^2.1.0",
        "phpstan/phpstan": "^0.9.2"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Elightwalk\\AuthGuard\\Providers\\AuthGuardServiceProvider"
            ],
            "aliases": {

            },
            "dont-discover": [
                "laravel/dusk"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Elightwalk\\AuthGuard\\": ""
        }
    }
}
